{
  "version": 2,
  "buildCommand": "npm run generate:env && npm run build:prod",
  "outputDirectory": "dist/template/browser",
  "framework": null,
  "installCommand": "npm install",
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
